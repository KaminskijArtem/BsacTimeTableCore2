<thead>
    <tr>
        <th rowspan="2"></th>
        <th colspan="7">Понедельник (@ViewBag.DateFrom.ToString("dd.MM.yyyy"))</th>
        <th colspan="7">Вторник (@ViewBag.DateFrom.AddDays(1).ToString("dd.MM.yyyy"))</th>
        <th colspan="7">Среда (@ViewBag.DateFrom.AddDays(2).ToString("dd.MM.yyyy"))</th>
        <th colspan="7">Четверг (@ViewBag.DateFrom.AddDays(3).ToString("dd.MM.yyyy"))</th>
        <th colspan="7">Пятница (@ViewBag.DateFrom.AddDays(4).ToString("dd.MM.yyyy"))</th>
    </tr>
    <tr>
        @for(var i = 1; i < 6; i++)
        {
            @for(var j = 1; j < 8; j++)
            {
                <th>@j</th>
            }
        }
    </tr>
</thead>