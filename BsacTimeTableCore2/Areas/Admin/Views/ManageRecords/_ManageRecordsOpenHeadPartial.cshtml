<thead>
    <tr>
        <th rowspan="2"></th>
        <th colspan="7">Понедельник</th>
        <th colspan="7">Вторник</th>
        <th colspan="7">Среда</th>
        <th colspan="7">Четверг</th>
        <th colspan="7">Пятница</th>
    </tr>
    <tr>
        @for(var i = 1; i < 6; i++)
        {
            @for(var j = 1; j < 8; j++)
            {
                <th>@j</th>
            }
        }
    </tr>
</thead>