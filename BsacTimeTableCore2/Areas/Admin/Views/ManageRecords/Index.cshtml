@model BsacTimeTableCore2.Data.DBModels.Faculty


@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>
<div class="row">
    <div class="col-md-4">
        <form asp-action="Open" method="get">
            <div class="form-group">
                <label asp-for="Id" class="control-label"></label>
                <select asp-for="Id" class="form-control" name="id" asp-items="ViewBag.FacultyId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Open" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>
